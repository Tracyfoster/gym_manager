<p id="notice"><%= notice %></p>

<h1>Imprests</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Item</th>
      <th>Debit</th>
      <th>Credit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imprests.each do |imprest| %>
      <tr>
        <td><%= imprest.date %></td>
        <td><%= imprest.item %></td>
        <td><%= imprest.debit %></td>
        <td><%= imprest.credit %></td>
        <td><%= link_to 'Show', imprest %></td>
        <td><%= link_to 'Edit', edit_imprest_path(imprest) %></td>
        <td><%= link_to 'Destroy', imprest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Imprest', new_imprest_path %>
